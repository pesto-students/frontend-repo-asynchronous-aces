{
	"name": "mantine-admin",
	"version": "1.0.0",
	"author": "Joshua Lee <jotyy318@gmail.com>",
	"description": "A Modern Admin Dashboard made with Mantine/React/NextJS!",
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,md}": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"scripts": {
		"dev": "next dev",
		"start": "next start",
		"build": "next build",
		"lint": "biome lint",
		"format": "biome format --write .",
		"test": "jest",
		"test:watch": "jest --watch",
		"type-check": "tsc --noEmit",
		"release": "standard-version",
		"push-release": "git push --follow-tags origin main",
		"pull": "git rebase origin main -i",
		"pre-commit": "bun run lint",
		"prepare": "husky install"
	},
	"dependencies": {
		"@hookform/resolvers": "3.3.4",
		"@mantine/core": "7.12.0",
		"@mantine/dates": "7.12.0",
		"@mantine/dropzone": "7.12.0",
		"@mantine/hooks": "7.12.0",
		"@mantine/modals": "7.12.0",
		"@mantine/notifications": "7.12.0",
		"@mantine/nprogress": "7.12.0",
		"@tabler/icons-react": "2.47.0",
		"@tanstack/react-query": "5.18.1",
		"@tanstack/react-table": "8.20.1",
		"@vercel/analytics": "1.3.1",
		"chart.js": "4.4.3",
		"dayjs": "1.11.12",
		"ky": "1.5.0",
		"mantine-react-table": "2.0.0-alpha.5",
		"next": "14.1.1",
		"react": "18.2.0",
		"react-chartjs-2": "5.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "7.50.0",
		"react-use": "17.5.0",
		"stylis-plugin-rtl": "2.1.1",
		"zod": "3.23.8",
		"zustand": "4.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "18.6.0",
		"@commitlint/config-conventional": "18.6.0",
		"@testing-library/dom": "9.3.4",
		"@testing-library/jest-dom": "6.4.1",
		"@testing-library/react": "14.2.1",
		"@testing-library/user-event": "14.5.2",
		"@types/jest": "29.5.12",
		"@types/node": "20.11.16",
		"@types/react": "18.2.52",
		"husky": "9.0.10",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"lint-staged": "15.2.1",
		"postcss": "8.4.41",
		"postcss-preset-mantine": "1.17.0",
		"postcss-simple-vars": "7.0.1",
		"prettier": "3.3.3",
		"standard-version": "9.5.0",
		"typescript": "5.3.3"
	}
}
